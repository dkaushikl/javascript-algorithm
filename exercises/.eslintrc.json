{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-empty": "off",
        "no-irregular-whitespace": "off",
        "require-jsdoc": "off",
        "linebreak-style": "off",
        "indent": ["error", 4],
        "object-curly-spacing": "off",
        "max-len": "off"
    }
}